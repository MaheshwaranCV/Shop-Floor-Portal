<mvc:View controllerName="SHOP_FLOOR_PORTAL_OF_MAHESHSHOP_FLOOR_PORTAL_OF_MAHESH.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<App id="App">
		<Page id="_IDGenPage1" title="SHOP FLOOR PORTAL - LOGIN" class="bg1">
			<content>
				<VBox id="_IDGenVBox1" justifyContent="Center" alignContent="Center" class="form">
					<items>
						<Title id="_IDGenTitle1" class="title" text="ENTER YOUR LOGIN CREDENTIALS"/>
						<Label id="_IDGenLabel1" class="label" text="USER ID"/>
						<Input width="20rem" id="uid" placeholder="User ID"></Input>
						<Label id="_IDGenLabel2" class="label" text="PASSWORD"/>
						<Input width="20rem" id="pasw" placeholder="Password" type="Password"></Input>
						<Button id="_IDGenButton1" width="20rem" text="LOGIN" icon="sap-icon://visits" type="Accept" press="onLogin"></Button>
					</items>
				</VBox>
			</content>
		</Page>
	</App>
</mvc:View>